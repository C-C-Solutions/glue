# Environment
NODE_ENV=development

# API Server
PORT=3000

# Database
# Note: When using Dev Container, these values are pre-configured
MONGODB_URI=mongodb://localhost:27017/glue

# Queue
REDIS_URL=redis://localhost:6379

# Logging
LOG_LEVEL=info

# AWS Configuration (LocalStack for local development)
# For dev container: use http://localstack:4566
# For local development: use http://localhost:4566
AWS_ENDPOINT_URL=http://localhost:4566
AWS_DEFAULT_REGION=us-east-1
AWS_ACCESS_KEY_ID=test
AWS_SECRET_ACCESS_KEY=test

# S3 Configuration
S3_BUCKET=glue-test-bucket
S3_ENDPOINT=http://localhost:4566
